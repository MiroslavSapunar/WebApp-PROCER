{
  "kind": "singleType",
  "collectionName": "nav_bars",
  "info": {
    "singularName": "nav-bar",
    "pluralName": "nav-bars",
    "displayName": "NavBar",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "logo_svg": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "categorias": {
      "type": "component",
      "repeatable": true,
      "component": "navbar.categoria-menu",
      "required": true
    },
    "boton_link": {
      "type": "component",
      "repeatable": false,
      "component": "elementos.boton-link"
    },
    "background_color": {
      "type": "enumeration",
      "enum": [
        "Croma_Principal_Oscuro",
        "Croma_Principal_Claro",
        "Croma_Secundario_Oscuro",
        "Croma_Secundario_Claro",
        "Medio_Tono_Oscuro",
        "Medio_Tono_Claro",
        "Blanco",
        "Negro",
        "Transparente"
      ],
      "required": true,
      "default": "Croma_Principal_Oscuro"
    },
    "estilo_texto_categorias": {
      "type": "component",
      "repeatable": false,
      "component": "elementos.estilo-texto",
      "required": true
    }
  }
}
